@* Generator: MvcView *@
@using Locust.Extensions
@using Locust.WebExtensions

@{
    ViewBag.PageTitle = "Browser Capabilities";
}

<h2>Browser Capabilities</h2>

<table class="table table-bordered table-condensed table-hover table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Request.Browser.ToDictionary())
        {
            <tr>
                <td>@item.Key</td>
                <td>@item.Value</td>
            </tr>
        }
    </tbody>
</table>

